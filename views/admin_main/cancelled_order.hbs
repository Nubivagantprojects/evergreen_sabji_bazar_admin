{{!-- {% load static %} --}}
<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
   <meta name="description" content="Evergreen Sabji Bazar offers fresh vegetables, fruits, and groceries in Hazaribagh. Order online for home delivery or pickup at your convenience." />
  <meta name="keywords" content="Evergreen Sabji Bazar, Hazaribagh, fresh vegetables, fruits, groceries, online vegetable store, home delivery, fresh produce" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Evergreen Sabji Bazar | Shop | Admin</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="../static/admin_login/plugins/fontawesome-free/css/all.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
  <!-- DataTables -->
  <link rel="stylesheet" href="../static/admin_login/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
  <link rel="stylesheet" href="../static/admin_login/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="../static/admin_login/dist/css/adminlte.min.css">
  <!-- Google Font: Source Sans Pro -->
  <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
  <!-- Favicon -->
  <link rel="shortcut icon" type="image" href="../static/admin_login/dist/img/favicon.png">
</head>
<body class="hold-transition sidebar-mini">
<div class="wrapper">
  <!-- Navbar -->
  <!-- Navbar -->
  <nav class="main-header navbar navbar-expand navbar-warning navbar-light">
    <!-- Left navbar links -->
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
      </li>
    </ul>

    

    <!-- Right navbar links -->

  </nav>
  <!-- /.navbar -->

  <!-- Main Sidebar Container -->
  <aside class="main-sidebar sidebar-dark-primary elevation-4">
  <!-- Brand Logo -->
  <a href="/" class="brand-link">
    <img src="../static/admin_login/dist/img/file.png" alt="Plant Nursary Kolkata" class="brand-image img-circle elevation-3"
    style="opacity: .8">
    <!-- <i class="fal fa-file-chart-pie"></i> -->    <span class="brand-text font-weight-light">EvergreenSabjiBazar</span>
  </a>
  <!-- Sidebar -->
  <div class="sidebar">
    <!-- Sidebar user panel (optional) -->
    <div class="user-panel mt-3 pb-3 mb-3 d-flex">
      <div class="info">
        <a href="#" class="d-block">{{user.first_name}}</a>
      </div>
    </div>
    <!-- Sidebar Menu -->
    <nav class="mt-2">
      <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
        <!-- Add icons to the links using the .nav-icon class
        with font-awesome or any other icon font library -->
        <li class="nav-item has-treeview">
          <a href="#" class="nav-link">
            <i class="nav-icon fas fa-th"></i>
            <p>
              Items
              <i class="right fas fa-angle-left"></i>
            </p>
          </a>
          <ul class="nav nav-treeview">
            <li class="nav-item">
              <a href="product_entry" class="nav-link">
                <i class="fas fa-edit nav-icon"></i>
                <p>Item Entry</p>
              </a>
            </li>
            <li class="nav-item">
              <a href="product_view" class="nav-link">
                <i class="fas fa-table nav-icon"></i>
                <p>Item View</p>
              </a>
            </li>
          </ul>
        </li>

        <!-- Orders Nav Item -->
        <li class="nav-item has-treeview menu-open">
          <a href="#" class="nav-link active">
            <i class="nav-icon fas fa-th"></i>
            <p>
              Orders
              <i class="right fas fa-angle-left"></i>
            </p>
          </a>
          <ul class="nav nav-treeview">
            {{!-- <li class="nav-item">
              <a href="unverified_order" class="nav-link">
                <i class="far fa-circle nav-icon text-warning"></i>
                <p>Unverified Orders</p>
              </a>
            </li> --}}
            <li class="nav-item">
              <a href="verified_order" class="nav-link">
                <i class="far fa-circle nav-icon text-info"></i>
                <p>Verified Orders</p>
              </a>
            </li>
            <li class="nav-item">
              <a href="in_transit_order" class="nav-link">
                <i class="far fa-circle nav-icon"></i>
                <p>In-Transit Orders</p>
              </a>
            </li>
            <li class="nav-item">
              <a href="delivered_order" class="nav-link">
                <i class="far fa-circle nav-icon text-success"></i>
                <p>Delivered Orders</p>
              </a>
            </li>
            <li class="nav-item">
              <a href="cancelled_order" class="nav-link active">
                <i class="far fa-circle nav-icon text-danger"></i>
                <p>Cancelled Orders</p>
              </a>
            </li>
          </ul>
        </li>
        <li class="nav-header d-flex"></li>
         
      </ul>
    </nav>
    <!-- /.sidebar-menu -->
  </div>
  <!-- /.sidebar -->
</aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>All Cancelled Orders List View</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              
              <li class="breadcrumb-item active">Cancelled Orders</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-12">
          <div class="card">
            <div class="card-header">
              <h3 class="card-title">All Cancelled Orders DataTable</h3>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                  <tr>
                    <th>Order Id</th>
                    <th>Order Amount</th>
                    <th>Phone No.</th>
                    <th>Payment</th>
                    <th>Address</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  {{!-- {% for item in items %} --}}
                  {{#each details}}
                  <tr>
                    <td>#{{this.id}}</td>
                    <td>â‚¹ {{this.total}}/-</td>
                    <td>
                      <div class="sparkbar" data-color="#00a65a" data-height="20">{{this.phone}} , {{this.phone2}}</div>
                    </td>
                    {{!-- {% if item.payment_method == 'COD' %} --}}
                    {{#ifs this.payment_method "COD"}}
                    <td><span class="badge badge-warning">Cash On Delivery</span></td>
                    {{!-- {% elif item.payment_method == 'RPAY' %} --}}
                    {{else ifs this.payment_method "RPAY" }}
                    <td><span class="badge badge-success">Online Payment</span></td>
                    {{!-- {% endif %} --}}
                    {{/ifs}}
                    <td>{{this.address}}</td>
                    <td><a href="/admin_main/Refund?id={{this.id}}&n=c"><button type="download-report" class="btn btn-danger">Refund</button></a></td>
                  </tr>
                  {{!-- {% endfor %} --}}
                  {{/each}}
                </tbody>
              </table>
            </div>
            <!-- /.card-body -->
          </div>
          <!-- /.card -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
  </aside>
  <!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->

<!-- jQuery -->
<script src="../static/admin_login/plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap 4 -->
<script src="../static/admin_login/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- DataTables -->
<script src="../static/admin_login/plugins/datatables/jquery.dataTables.min.js"></script>
<script src="../static/admin_login/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
<script src="../static/admin_login/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
<script src="../static/admin_login/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
<!-- AdminLTE App -->
<script src="../static/admin_login/dist/js/adminlte.min.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="../static/admin_login/dist/js/demo.js"></script>
<!-- page script -->
<script>
  $(function () {
    $("#example1").DataTable({
      "responsive": true,
      "autoWidth": false,
    });
    $('#example2').DataTable({
      "paging": true,
      "lengthChange": false,
      "searching": false,
      "ordering": true,
      "info": true,
      "autoWidth": false,
      "responsive": true,
    });
  });
</script>
<script type="text/javascript">
  document.addEventListener("contextmenu",function(mydocument) {
      mydocument.preventDefault();
  });
</script>
</body>
</html>
