</aside>


  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-dark">Item Entry</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              
              <li class="breadcrumb-item active">Item Entry</li>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <div class="container">
      {{#if err }}
      {{#each details}}
      <h4><strong><font color="red">{{message}}</font></strong><h4>
      {{/each }}
      {{else}}
      {{#each details}}
      <h4><strong><font color="green">{{message}}</font></strong><h4>
      {{/each }}
      {{/if}}
    </div>
    <!-- /.content-header -->


    <section class="content">
      <div class="container-fluid">
          <div class="row">
            <div class="col-md-12">
                <div class="card card-primary">
                  <div class="card-header">
                    <h3 class="card-title">Shipment Total Info</h3>
                    <div class="card-tools">
                      <button type="button" class="btn btn-warning" data-card-widget="collapse"><strong><i
                            class="fas fa-minus" style="color:#FFF;"></i></strong></button>
    
                    </div>
                  </div>
                  <!-- /.card-header -->
                  
    
                  <div class="card-body">
                    <form action="/admin_main/transit_entry_submit?id={{id}}" method="post" enctype="application/json">
                      <div class="form-group">
                        <label for="item_name">Shipper Name<font color="red">*</font></label>
                        <input type="text" class="form-control" name="ship_name" placeholder="Enter Category Name"  id="ship_name" value="{{details.ship_partner}}" required>
                        <span id="errsitid" class="text-danger font-weight-bold"></span>
                      </div>
                      <div class="form-group">
                        <label for="item_name">Transit Id<font color="red">*</font></label>
                        <input type="text" class="form-control" name="ship_id" placeholder="Enter Category Name"  id="ship_id" value="{{details.track_id}}" required>
                        <span id="errsitid" class="text-danger font-weight-bold"></span>
                      </div>
                      <div class="form-group">
                        <label for="item_name">Transit Url<font color="red">*</font></label>
                        <input type="text" class="form-control" name="ship_url" placeholder="Enter Category Name"  id="ship_url" value="{{details.track_url}}" required>
                        <span id="errsitid" class="text-danger font-weight-bold"></span>
                      </div>
                      <div class="row">
                      <button type="submit" class="changepassword-submit-btn">Save Item</button>
                      </div>
                    </form>
                  </div>
                  
                  
                </div>
              </div>
          
          <!-- /.col -->
          <!-- /.col -->
        </div>
      </div>
    </section>  
  <!-- /.content-wrapper -->
















  

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
  </aside>
  <!-- /.control-sidebar -->
</div>
